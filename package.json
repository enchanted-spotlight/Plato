{
  "name": "plato",
  "version": "0.0.1",
  "description": "note-taking app",
  "main": "server.js",
  "scripts": {
    "start": "npm run lint && nodemon server/server.js",
    "test": "mocha --require ./test/setup.js --compilers jsx:babel-register -R list",
    "lint": "eslint --ext .js --ext .jsx test server db public --ignore-path client/public/bundle.js",
    "watch": "babel --watch ./public --out-dir ./public/compiled --presets=es2015,react --ignore=node_modules,compiled --source-maps inline && node server/server.js",
    "build": "npm run lint && node_modules/.bin/webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enchanted-spotlight/Plato.git"
  },
  "keywords": [
    "tldr",
    "speech",
    "to",
    "text",
    "time",
    "marked",
    "notes",
    "drawlings"
  ],
  "author": "Clarabelle Cheng-Yue, Jon Garrett, Daniel Snyder, Joseph Yu",
  "license": "MIT License",
  "bugs": {
    "url": "https://github.com/enchanted-spotlight/Plato/issues"
  },
  "homepage": "https://github.com/enchanted-spotlight/Plato#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "mongoose": "^4.6.1",
    "nodemon": "^1.10.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "superagent": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "react-addons-test-utils": "^15.3.2",
    "supertest": "^2.0.0",
    "webpack": "^1.13.2"
  }
}
